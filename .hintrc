{
  "extends": ["development"],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 18.3",
    "not safari <= 18.3",
    "not chrome <= 109",
    "not samsung <= 27"
  ],
  "hints": {
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-size-adjust",
          "-webkit-overflow-scrolling",
          "scrollbar-width",
          "scrollbar-width: thin",
          "scrollbar-width: thin"
        ]
      }
    ],
    "no-inline-styles": "off",
    "css-prefix-order": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ]
  }
}
