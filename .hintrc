{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 18.3",
    "not safari <= 18.3"
  ],
  "hints": {
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-size-adjust",
          "-webkit-overflow-scrolling"
        ]
      }
    ],
    "no-inline-styles": "off",
    "css-prefix-order": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ]
  }
}